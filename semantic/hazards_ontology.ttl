@prefix : <http://hazard-ontology.org/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .

#################################################################
#    Ontology declaration
#################################################################

: a owl:Ontology ;
    rdfs:label "South Tyrol Avalanche and Landslides Hazard Management Ontology"@en ;
    rdfs:comment "An ontology for managing hazard zones, road segments, and municipalities in South Tyrol"@en .

#################################################################
#    Classes
#################################################################

:SpatialEntity a owl:Class ;
    rdfs:label "Spatial Entity"@en ;
    rdfs:comment "A general spatial feature or area, including both administrative units and hazard zones."@en .

:GeographicUnit a owl:Class ;
    rdfs:subClassOf :SpatialEntity ;
    rdfs:label "Geographic unit"@en ;
    rdfs:comment "An administrative, geographical or territorial entity that spans a certain area. For example, a municipality, a country, a district, etc."@en .

:Municipality a owl:Class ;
    rdfs:subClassOf :GeographicUnit ;
    rdfs:label "Municipality"@en ;
    rdfs:comment "An administrative territorial entity in South Tyrol"@en .

:ProvincialDistrict a owl:Class ;
    rdfs:subClassOf :GeographicUnit ;
    rdfs:label "Provincial District"@en ;
    rdfs:comment "Administrative district containing multiple municipalities"@en .

:HealthDistrict a owl:Class ;
    rdfs:subClassOf :GeographicUnit ;
    rdfs:label "Health District"@en ;
    rdfs:comment "Smaller healthcare administrative region in the province of South Tyrol"@en .

:HealthRegion a owl:Class ;
    rdfs:subClassOf :GeographicUnit ;
    rdfs:label "Health Region"@en ;
    rdfs:comment "Larger Healthcare administrative region in the province of South Tyrol"@en .

:HazardZone a owl:Class ;
    rdfs:subClassOf :SpatialEntity ;
    rdfs:label "Hazard zone"@en ;
    rdfs:comment "A spatial area subject to a specific hazard."@en .

:AvalancheZone a owl:Class ;
    rdfs:subClassOf :HazardZone ;
    rdfs:label "Avalanche zone"@en .

:LandslideZone a owl:Class ;
    rdfs:subClassOf :HazardZone ;
    rdfs:label "Landslide zone"@en .

:RoadSegment a owl:Class ;
    rdfs:label "Road segment"@en .

:Bridge a owl:Class ;
    rdfs:subClassOf :RoadSegment ;
    rdfs:label "Bridge"@en .

:Tunnel a owl:Class ;
    rdfs:subClassOf :RoadSegment ;
    rdfs:label "Tunnel"@en .

:RouteReference a owl:Class ;
    rdfs:label "Route Reference"@en .

:RoadSegmentConnectionPoint a owl:Class ;
    rdfs:label "Road Segment Connection Point"@en .

:HazardProcess a owl:Class ;
    owl:oneOf (
        <http://hazard-ontology.org/Landslide>
        <http://hazard-ontology.org/Collapse>
        <http://hazard-ontology.org/Sliding>
        <http://hazard-ontology.org/Subsidence>
        <http://hazard-ontology.org/SlopeFlow>
        <http://hazard-ontology.org/Avalanche>
        <http://hazard-ontology.org/SnowSlabAvalanche>
        <http://hazard-ontology.org/PowderSnowAvalanche>
        <http://hazard-ontology.org/SurfaceAvalanche>
    ) ;
    rdfs:label "Process"@en ;
    rdfs:comment "A typification of a hazard process."@en .

:StudyLevel a owl:Class ;
    owl:oneOf (
        <http://hazard-ontology.org/HighStudyLevel>
        <http://hazard-ontology.org/MediumStudyLevel>
        <http://hazard-ontology.org/LowStudyLevel>
    ) ;
    rdfs:label "Study Level"@en ;
    rdfs:comment "The level of study or investigation for a hazard zone."@en .

:DangerLevel a owl:Class ;
    rdfs:label "Danger Level"@en ;
    rdfs:comment "The level of danger associated with a hazard zone."@en .

:AvalancheDangerLevel a owl:Class ;
    owl:oneOf (
        <http://hazard-ontology.org/VeryHighAvalancheDanger>
        <http://hazard-ontology.org/HighAvalancheDanger>
        <http://hazard-ontology.org/MediumAvalancheDanger>
        <http://hazard-ontology.org/ExaminedNotInDangerAvalanche>
    ) ;
    rdfs:subClassOf :DangerLevel ;
    rdfs:label "Avalanche Danger Level"@en .

:LandslideDangerLevel a owl:Class ;
    owl:oneOf (
        <http://hazard-ontology.org/VeryHighLandslideDanger>
        <http://hazard-ontology.org/HighLandslideDanger>
        <http://hazard-ontology.org/MediumLandslideDanger>
        <http://hazard-ontology.org/ExaminedNotInDangerLandslide>
    ) ;
    rdfs:subClassOf :DangerLevel ;
    rdfs:label "Landslide Danger Level"@en .

#################################################################
#    Object Properties
#################################################################

:isPartOfGeographicUnit a owl:ObjectProperty ;
    rdfs:label "is part of geographic unit"@en ;
    rdfs:domain :GeographicUnit ;
    rdfs:range :GeographicUnit .

:hasGeometry a owl:ObjectProperty,
               owl:FunctionalProperty ;
    rdfs:domain :SpatialEntity ;
    rdfs:range geo:Geometry ;
    rdfs:label "has geometry"@en .

:isLocatedInMunicipality a owl:ObjectProperty ;
    rdfs:domain :SpatialEntity ;
    rdfs:range :Municipality ;
    rdfs:label "is located in municipality"@en ;
    owl:inverseOf :hasSpatialEntity .

:hasSpatialEntity a owl:ObjectProperty ;
    rdfs:domain :Municipality ;
    rdfs:range :SpatialEntity ;
    rdfs:label "has spatial entity"@en ;
    owl:inverseOf :isLocatedInMunicipality .

:hasHazardProcess a owl:ObjectProperty ,
               owl:FunctionalProperty ;
    rdfs:domain :HazardZone ;
    rdfs:range :HazardProcess ;
    rdfs:label "has process"@en .

:hasStudyLevel a owl:ObjectProperty ,
               owl:FunctionalProperty ;
    rdfs:domain :HazardZone ;
    rdfs:range :StudyLevel ;
    rdfs:label "has study level"@en .

:hasDangerLevel a owl:ObjectProperty ,
               owl:FunctionalProperty ;
    rdfs:domain :HazardZone ;
    rdfs:range :DangerLevel ;
    rdfs:label "has danger level"@en .

:belongsToRef a owl:ObjectProperty ;
    rdfs:domain :RoadSegment ;
    rdfs:range :RouteReference ;
    rdfs:label "belongs to route reference"@en .

:comesFromPoint a owl:ObjectProperty ,
               owl:FunctionalProperty ;
    rdfs:domain :RoadSegment ;
    rdfs:range :RoadSegmentConnectionPoint ;
    rdfs:label "comes from point"@en .

:goesToPoint a owl:ObjectProperty ,
               owl:FunctionalProperty ;
    rdfs:domain :RoadSegment ;
    rdfs:range :RoadSegmentConnectionPoint ;
    rdfs:label "goes to point"@en .

:intersectsHazardZone a owl:ObjectProperty ;
    rdfs:domain :RoadSegment ;
    rdfs:range :HazardZone ;
    rdfs:label "intersects hazard zone"@en ;
    owl:inverseOf :isIntersectedByRoadSegment .

:isIntersectedByRoadSegment a owl:ObjectProperty ;
    rdfs:domain :HazardZone ;
    rdfs:range :RoadSegment ;
    rdfs:label "is intersected by road segment"@en ;
    owl:inverseOf :intersectsHazardZone .

#################################################################
#    Data Properties
#################################################################

:name a owl:DatatypeProperty ;
    rdfs:domain :SpatialEntity ;
    rdfs:range xsd:string ;
    rdfs:label "name"@en ;
    rdfs:comment "Name of the entity in different languages"@en .

:hasIdentifier a owl:DatatypeProperty ;
    rdfs:domain :SpatialEntity ;
    rdfs:range xsd:string ;
    rdfs:label "has an identifier"@en .

:hasIstatCode a owl:DatatypeProperty ,
                owl:FunctionalProperty ;
    rdfs:subPropertyOf :hasIdentifier ;
    rdfs:domain :Municipality ;
    rdfs:label "ISTAT code"@en .

:hasBelfioreCode a owl:DatatypeProperty ,
                    owl:FunctionalProperty ;
    rdfs:subPropertyOf :hasIdentifier ;
    rdfs:domain :Municipality ;
    rdfs:label "Belfiore code"@en .

:hasZipCode a owl:DatatypeProperty ,
              owl:FunctionalProperty ;
    rdfs:subPropertyOf :hasIdentifier ;
    rdfs:domain :Municipality ;
    rdfs:label "ZIP code"@en .

:hasOsmId a owl:DatatypeProperty ;
    rdfs:domain :RoadSegment ;
    rdfs:range xsd:string ;
    rdfs:label "OSM id"@en .

:hasArea a owl:DatatypeProperty ,
            owl:FunctionalProperty ;
    rdfs:domain :SpatialEntity ;
    rdfs:range xsd:decimal ;
    rdfs:label "area in square meters"@en .

# GeoSPARQL geometry
geo:asWKT rdfs:label "as WKT"@en .

#################################################################
#    Individuals (Instances)
#################################################################

# Process types
<http://hazard-ontology.org/HazardProcess/Landslide> a :HazardProcess ;
